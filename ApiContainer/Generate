. Load

Name=$1

Start

if [[ $Name == "" ]]; then 
    Extract
else
    find / -type d -name $Name 2>/dev/null |
    while read dir;
    do
        if [ -f $dir/Database.json ]; then
            cd $dir
            Generate
        fi
    done
fi

if [ ! -f $PWD/Database.json ]; then
    Warning "Did not found $PWD/Database.json";
    exit;
fi

export Directory=$PWD

cd /PaydarCore/Generation

if [ -z ${IsBuilt+x} ] && [ ! -d /PaydarCore/Generation/bin ]; then
    dotnet build /PaydarCore/Generation
    chmod -R 777 /PaydarCore/Generation
fi

dotnet run $Directory $Organization $Repository

prompt=$(sudo -nv 2>&1)
if [ $? -eq 0 ]; then
    sudo chmod -R 777 $Directory
elif echo $prompt | grep -q '^sudo:'; then
    sudo chmod -R 777 $Directory
else
    chmod -R 777 $Directory
fi

if [[ $RunMigration != False ]]; then
    Migrate
fi

Stop